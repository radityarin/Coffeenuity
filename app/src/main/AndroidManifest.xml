<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ftp.coffeenuity">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@drawable/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_logo"
        android:supportsRtl="true"
        android:theme="@style/Theme.Coffeenuity">
        <activity
            android:name=".presentation.admin.AdminMainActivity"
            android:exported="true" />
        <activity
            android:name=".presentation.auth.register.RegisterActivity"
            android:exported="true" />
        <activity
            android:name=".presentation.auth.onboarding.OnBoardingActivity"
            android:exported="true"/>
        <activity
            android:name=".presentation.auth.login.LoginActivity"
            android:exported="true" />
        <activity
            android:name=".presentation.auth.splash.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.MainActivity"
            android:exported="true" />
    </application>

</manifest>